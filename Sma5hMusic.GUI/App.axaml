<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:cv="clr-namespace:Sma5hMusic.GUI.Converters;assembly=Sma5hMusic.GUI"
             xmlns:local="clr-namespace:Sma5hMusic.GUI"
             x:Class="Sma5hMusic.GUI.App">
  <Application.DataTemplates>
    <local:ViewLocator/>
  </Application.DataTemplates>
  <Application.Resources>
    <cv:SeriesNameConverter x:Key="SeriesNameConverter"/>
    <cv:MSBTValueConverter x:Key="MSBTValueConverter"/>
    <cv:RecordTypeConverter x:Key="RecordTypeConverter"/>
    <cv:FilePathConverter x:Key="FilePathConverter"/>
    <cv:LocaleNameConverter x:Key="LocaleNameConverter"/>
    <cv:HiddenColumnConverter x:Key="HiddenColumnConverter"/>
    <cv:LabelValueConverter x:Key="LabelValueConverter"/>
  </Application.Resources>
  <Application.Styles>
    <StyleInclude Source="/Assets/Styles/Styles.xaml" />
  </Application.Styles>
</Application>
